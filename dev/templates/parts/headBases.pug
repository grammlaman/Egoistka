.head-cat-title
	h3!= catTitle
.head-cat-units
	each base in belie
		a(href='#').head-cat-unit
			.head-cat-photo
				+img(base['image'])
			.head-cat-name
				span!= base['title']
				svg(xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24")
					path(d="M12 4l-1.41 1.41L16.17 11H4v2h12.17l-5.58 5.59L12 20l8-8-8-8z")